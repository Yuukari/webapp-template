{
  "name": "webapp-template",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "start": "node build/server.js",
    "server": "node ace serve --watch",
    "client": "node ace mix:watch",
    "build": "npm run client:build && npm run server:build",
    "server:build": "node ace build --production",
    "client:build": "node ace mix:build --production",
    "dev": "concurrently \"npm run server\" \"npm run client\""
  },
  "devDependencies": {
    "@adonisjs/assembler": "^5.7.0",
    "@babel/preset-react": "^7.17.12",
    "@japa/preset-adonis": "^1.0.16",
    "@japa/runner": "^2.0.8",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.5.7",
    "adonis-preset-ts": "^2.1.0",
    "babel-loader": "^8.2.5",
    "concurrently": "^7.2.1",
    "laravel-mix": "^6.0.44",
    "pino-pretty": "^8.0.0",
    "react-refresh": "^0.13.0",
    "sass": "^1.52.1",
    "sass-loader": "^12.1.0",
    "typescript": "~4.6",
    "youch": "^3.2.0",
    "youch-terminal": "^2.1.4"
  },
  "dependencies": {
    "@adonisjs/core": "^5.8.3",
    "@adonisjs/repl": "^3.1.11",
    "@adonisjs/session": "^6.4.0",
    "@adonisjs/shield": "^7.0.11",
    "@adonisjs/view": "^6.1.5",
    "@blueprintjs/core": "^4.4.0",
    "@blueprintjs/datetime": "^4.2.3",
    "adonis-mix-asset": "^3.0.0",
    "normalize.css": "^8.0.1",
    "proxy-addr": "^2.0.7",
    "react": "^18.1.0",
    "react-dom": "^18.1.0",
    "react-query": "^3.39.1",
    "reflect-metadata": "^0.1.13",
    "source-map-support": "^0.5.21",
    "teaful": "^0.10.0",
    "teaful-devtools": "^0.5.1"
  }
}
